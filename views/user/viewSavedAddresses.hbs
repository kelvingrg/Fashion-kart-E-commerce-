

  <div class="row">
    <div class="col-md-2">&nbsp;</div>
    <div class="col-md-8">
      <div class="row space-16">&nbsp;</div>
      <div class="row">


      {{#each addressData.[0].address}} 

  <div class="col-sm-4">
          <div class="thumbnail ">
            <div class="caption text-left ml-3">
              <h4>{{this.firstName}}&nbsp;{{this.lastName}} </h4>
            </div>
            <div class="caption card-footer ml-3 ">
              <ul >
                <li>{{this.address}}</li>
                <li>{{this.city}}</li>
                <li>{{this.country}}</li>
                <li>{{this.pinCode}}</li>
                <li>{{this.mobileNumber}}</li>
                <li>{{this.email}}</li>
                <li></li>
              </ul><br>
            <h3 onclick="deleteAddress('{{this._id}}')"><i class="pe-7s-trash"></i></h3>
            </div>
          </div>
        </div>

  {{/each}}

      </div>
      <div class="col-md-2">&nbsp;</div>
    </div>
  </div>






>